{
  "version": "1",
  "cognitoConfig": {
    "identityPoolName": "forwardsflow_identitypool",
    "allowUnauthenticatedIdentities": false,
    "resourceNameTruncated": "forwax",
    "userPoolName": "forwardsflow_userpool",
    "autoVerifiedAttributes": [
      "email"
    ],
    "mfaConfiguration": "OPTIONAL",
    "mfaTypes": [
      "TOTP"
    ],
    "smsAuthenticationMessage": "Your authentication code is {####}",
    "smsVerificationMessage": "Your verification code is {####}",
    "emailVerificationSubject": "Verify your ForwardsFlow account",
    "emailVerificationMessage": "Your verification code is {####}",
    "defaultPasswordPolicy": true,
    "passwordPolicyMinLength": 8,
    "passwordPolicyCharacters": [
      "Requires Lowercase",
      "Requires Numbers",
      "Requires Symbols",
      "Requires Uppercase"
    ],
    "requiredAttributes": [
      "email",
      "phone_number"
    ],
    "aliasAttributes": [],
    "userpoolClientGenerateSecret": false,
    "userpoolClientRefreshTokenValidity": 30,
    "userpoolClientWriteAttributes": [
      "email",
      "phone_number"
    ],
    "userpoolClientReadAttributes": [
      "email",
      "phone_number"
    ],
    "userpoolClientLambdaRole": "forwax",
    "userpoolClientSetAttributes": false,
    "sharedId": "forwax",
    "resourceName": "forwardsflowauth",
    "authSelections": "identityPoolAndUserPool",
    "useDefault": "default",
    "userPoolGroupList": [
      "Admins",
      "Banks",
      "Investors",
      "Borrowers"
    ],
    "serviceName": "Cognito",
    "usernameCaseSensitive": false,
    "useEnabledMfas": true
  }
}
